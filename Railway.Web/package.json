{
  "version": "1.0.0",
  "name": "asp.net",
  "private": true,
  "scripts": {
    "ngc": "ngc -p ./tsconfig-aot.json",
    "start": "concurrently \"webpack-dev-server --hot --inline --port 8080\" \"dotnet run\" ",
    "webpack-dev": "set NODE_ENV=development && webpack",
    "webpack-production": "set NODE_ENV=production && webpack",
    "build-dev": "npm run webpack-dev",
    "build-production": "npm run ngc && npm run webpack-production",
    "watch-webpack-dev": "set NODE_ENV=development && webpack --watch --color",
    "watch-webpack-production": "npm run build-production --watch --color",
    "publish-for-iis": "npm run build-production && dotnet publish -c Release"
  },
  "dependencies": {
    "@types/react-router-redux": "^5.0.0",
    "@types/react-tap-event-plugin": "0.0.30",
    "awayjs-full": "^0.2.39",
    "font-awesome": "4.7.0",
    "ie-shim": "0.1.0",
    "material-ui": "^0.17.4",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-redux": "5.0.3",
    "react-router": "^4.0.0",
    "react-router-dom": "4.0.0",
    "react-router-redux": "5.0.0-alpha.3",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "3.6.0",
    "redux-actions": "2.0.1",
    "redux-thunk": "^2.2.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "@types/history": "^4.5.0",
    "@types/material-ui": "^0.16.60",
    "@types/node": "7.0.8",
    "@types/react": "^15.0.20",
    "@types/react-dom": "^0.14.23",
    "@types/react-redux": "^4.4.38",
    "@types/react-router": "^4.0.2",
    "@types/react-router-dom": "^4.0.1",
    "@types/react-tap-event-plugin": "0.0.30",
    "@types/redux": "^3.6.0",
    "@types/redux-actions": "^1.2.3",
    "@types/redux-thunk": "^2.1.0",
    "@types/whatwg-fetch": "0.0.33",
    "awesome-typescript-loader": "3.1.2",
    "clean-webpack-plugin": "0.1.15",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.27.3",
    "file-loader": "0.10.1",
    "html-webpack-plugin": "2.28.0",
    "less": "2.7.2",
    "less-loader": "4.0.2",
    "raw-loader": "0.5.1",
    "source-map-loader": "0.2.0",
    "style-loader": "0.15.0",
    "ts-helpers": "^1.1.2",
    "ts-node": "~2.0.0",
    "typescript": "2.2.2",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.4.1"
  }
}
